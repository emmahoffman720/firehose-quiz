  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <div class = "col-xs-3">
      <div class="circle">
        <br />
          IDEAS
        <br />
          FOR THE
        <br />
          WEB
      </div>
    </div>
    <div class = "col-xs-4 slogan">
      This site was created to help you create yours
    </div>
    <div class = "col-xs-5 links">
      <%= link_to 'Contribute', '#' %>
      <%= link_to 'Layout', '#' %>
      <%= link_to 'About', '#' %>
  </header>

  <hr />

<br class = "clear" />

<%= yield %>

</body>
</html>
